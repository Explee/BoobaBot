{
  "env": {
    "browser": true,
    "node": true
  },
  "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "impliedStrict": true,
      "ecmaFeatures": {
          "jsx": true
      }
  },
  "extends": "eslint:recommended",
  "rules": {
      "semi": ["error", "always"],
      "no-console": "off"
  }
}